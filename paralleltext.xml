<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="projects" parallel="tests" thread-count="2">
	<test name="chrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="projectTestCases.LoginTestCases"></class>
		</classes>
	</test>
	<test name="edge">
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="projectTestCases.LoginTestCases"></class>
			<class name="projectTestCases.ManageExpenseTestCase"></class>
			<class name="projectTestCases.ManageUsersTestCase"></class>
			<class name="projectTestCases.ManageContentTestCase"></class>
			
		</classes>
	</test>
</suite> 